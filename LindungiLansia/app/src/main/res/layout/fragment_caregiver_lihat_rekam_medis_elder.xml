<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout10"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/primary_blue"
    tools:context=".CaregiverLihatRekamMedisElderFragment">

    <Button
        android:id="@+id/btnCaregiverLihatRekamMedisElderButtonKembali"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:drawableStart="@drawable/ic_round_arrow_back_ios_24_pure_white"
        android:fontFamily="sans-serif"
        android:text="Kembali"
        android:textAllCaps="false"
        android:textColor="@color/pure_white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.049"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:fontFamily="@font/sen" />

    <TextView
        android:id="@+id/tvCaregiverLihatRekamMedisElderJudul"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/sen"
        android:text="Rekam Medis"
        android:textColor="@color/pure_white"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.106" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="371dp"
        android:layout_height="659dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/rounder_corner_25"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvCaregiverLihatRekamMedisElderJudul">

        <TextView
            android:id="@+id/tvCaregiverLihatRekamMedisElderNamaElder"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            android:fontFamily="@font/sen_bold"
            android:text="Himejima Ujang"
            android:textColor="@color/jet_black"
            android:textSize="30sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.121"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <LinearLayout
            android:layout_width="334dp"
            android:layout_height="493dp"
            android:layout_marginTop="20dp"
            android:orientation="vertical"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tvCaregiverLihatRekamMedisElderNamaElder">

            <TextView
                android:id="@+id/tvCaregiverLihatRekamMedisElderJudulTTL"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/sen"
                android:text="Tempat Tanggal Lahir"
                android:textColor="@color/shadow_white"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/tvCaregiverLihatRekamMedisElderIsiTTL"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10sp"
                android:fontFamily="@font/sen"
                android:text="Yamanashi, 15 Agustus 1888"
                android:textColor="@color/jet_black"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tvCaregiverLihatRekamMedisElderJudulJenisKelamin"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/sen"
                android:text="Jenis Kelamin"
                android:textColor="@color/shadow_white"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/tvCaregiverLihatRekamMedisElderIsiJenisKelamin"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10sp"
                android:fontFamily="@font/sen"
                android:text="Laki-Laki"
                android:textColor="@color/jet_black"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tvCaregiverLihatRekamMedisElderJudulAlamat"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/sen"
                android:text="Alamat"
                android:textColor="@color/shadow_white"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/tvCaregiverLihatRekamMedisElderIsiAlamat"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10sp"
                android:fontFamily="@font/sen"
                android:text="Komplek Rhodes Island No.16, Wolumonde, Leithania"
                android:textColor="@color/jet_black"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tvCaregiverLihatRekamMedisElderJudulTelepon"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/sen"
                android:text="Telepon"
                android:textColor="@color/shadow_white"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/tvCaregiverLihatRekamMedisElderIsiTelepon"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10sp"
                android:fontFamily="@font/sen"
                android:text="081234567890"
                android:textColor="@color/jet_black"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tvCaregiverLihatRekamMedisElderJudulGolonganDarah"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:fontFamily="@font/sen"
                android:text="Golongan Darah"
                android:textColor="@color/shadow_white"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/tvCaregiverLihatRekamMedisElderIsiGolonganDarah"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10sp"
                android:fontFamily="@font/sen"
                android:text="A+"
                android:textColor="@color/jet_black"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tvCaregiverLihatRekamMedisElderJudulBeratBadan"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/sen"
                android:text="Berat Badan"
                android:textColor="@color/shadow_white"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/tvCaregiverLihatRekamMedisElderIsiBeratBadan"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10sp"
                android:fontFamily="@font/sen"
                android:text="57 kg"
                android:textColor="@color/jet_black"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tvCaregiverLihatRekamMedisElderJudulTinggiBadan"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/sen"
                android:text="Tinggi Badan"
                android:textColor="@color/shadow_white"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/tvCaregiverLihatRekamMedisElderIsiTinggiBadan"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10sp"
                android:fontFamily="@font/sen"
                android:text="166 cm"
                android:textColor="@color/jet_black"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tvCaregiverLihatRekamMedisElderJudulAlergi"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:fontFamily="@font/sen"
                android:text="Alergi"
                android:textColor="@color/shadow_white"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/tvCaregiverLihatRekamMedisElderIsiAlergi"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10sp"
                android:fontFamily="@font/sen"
                android:text="Selai Kacang"
                android:textColor="@color/jet_black"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/tvCaregiverLihatRekamMedisElderJudulRiwayatPenyakit"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/sen"
                android:text="Riwayat Penyakit"
                android:textColor="@color/shadow_white"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/tvCaregiverLihatRekamMedisElderIsiRiwayatPenyakit"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10sp"
                android:fontFamily="@font/sen"
                android:text="Skizofrenia, ADHD, Bipolar, MPD, PTSD"
                android:textColor="@color/jet_black"
                android:textSize="14sp" />

        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>